def fibonacci_sequence(n, sequence):
    if len(sequence) >= n:
        return sequence

    next_fib = sequence[len(sequence)-1] + sequence[len(sequence)-2]
    sequence=append(sequence, next_fib)

    return fibonacci_sequence(n, sequence)

n = 15
seq = [0, 1]
fib_sequence = fibonacci_sequence(n, seq)
print(fib_sequence)
